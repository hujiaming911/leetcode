class Solution {
    public int maxProfit(int[] prices) {
        if(prices.length<2){
            return 0;
        }
        int profit=0;
        int start=prices[0];
        for(int i=1;i<prices.length;i++){
            start=Math.min(start,prices[i]);
            profit=Math.max(profit,prices[i]-start);
        }
        return profit;
    }
}



