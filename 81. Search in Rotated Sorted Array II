class Solution {
    public boolean search(int[] nums, int target) {
        if(nums.length==0){
            return false;
        }
        Arrays.sort(nums);
        int start=0;
        int end=nums.length-1;
        int mid=0;
        while(start<=end){
            mid=start+(end-start)/2;
            if(nums[mid]==target){
                return true;
            }
            else if(nums[mid]>target){
                end=mid-1;
            }
            else{
                start=mid+1;
            }
        }
        return false;
    }
}
