public class Solution extends GuessGame {
    public int guessNumber(int n) {
        int start=1;
        int end=n;
        while(start<=end){
            int mid=(end-start)/2+start;
            if(guess(mid)==-1){
                end=mid-1;
            }else if(guess(mid)==1){
                start=mid+1;
            }else{
                return mid;
            }
        }
        return 123;
    }
}
